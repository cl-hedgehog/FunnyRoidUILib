<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension|reference"></attr>
        <attr name="roundProgressWidth" format="dimension|reference"></attr>
        <attr name="circleSize" format="dimension|reference"/>
        <attr name="textSize" format="dimension|reference"/>
        <attr name="textColor" format="color"/>
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="SpringPicker">
        <attr name="maxWidth" format="dimension|reference"/>
        <attr name="minWidth" format="dimension|reference"/>
        <attr name="pickerWidth" format="dimension|reference"/>
        <attr name="pickerGap" format="dimension|reference"/>
    </declare-styleable>
</resources>
